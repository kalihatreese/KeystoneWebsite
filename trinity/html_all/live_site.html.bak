    }

    .card-emphasis {
      font-size: 13px;
      color: var(--accent-strong);
      font-weight: 600;
      margin-top: 4px;
    }

    /* Super Santa + ReeseEffect */

    .santa-grid {
      display: grid;
      grid-template-columns: minmax(0, 1.4fr) minmax(0, 1.2fr);
      gap: 18px;
    }

    .santa-card {
      background: var(--bg-soft);
      border-radius: var(--radius-xl);
      border: 1px solid rgba(31, 41, 55, 0.9);
      padding: 16px 15px;
      box-shadow: var(--shadow-soft);
    }

    .santa-title {
      font-size: 15px;
      font-weight: 700;
      margin-bottom: 6px;
    }

    .santa-sub {
      font-size: 12px;
      color: var(--muted);
      margin-bottom: 10px;
    }

    .santa-form {
      display: flex;
      flex-direction: column;
      gap: 8px;
      margin-top: 6px;
    }

    .santa-label {
      font-size: 11px;
      color: var(--muted);
    }

    .santa-input,
    .santa-textarea {
      width: 100%;
      padding: 7px 9px;
      border-radius: 9px;
      border: 1px solid rgba(31, 41, 55, 0.95);
      background: #020617;
      color: var(--text);
      font-size: 12px;
      outline: none;
    }

    .santa-input:focus,
    .santa-textarea:focus {
      border-color: rgba(249, 115, 22, 0.9);
      box-shadow: 0 0 0 1px rgba(249, 115, 22, 0.3);
    }

    .santa-checkbox-row {
      display: flex;
      align-items: center;
      gap: 6px;
      font-size: 11px;
      color: var(--muted);
      margin-top: 4px;
    }

    .santa-checkbox {
      width: 13px;
      height: 13px;
      accent-color: var(--accent-strong);
    }

    .santa-small {
      font-size: 10px;
      color: var(--muted);
      margin-top: 4px;
    }

    .reese-test-area {
      margin-top: 8px;
    }

    .reese-output {
      margin-top: 8px;
      font-size: 12px;
      color: #e5e7eb;
      border-radius: 10px;
      border: 1px solid rgba(31, 41, 55, 0.9);
      background: radial-gradient(circle at top left, rgba(249,115,22,0.18), #020617);
      padding: 10px 11px;
      min-height: 40px;
      white-space: pre-wrap;
    }

    /* FAQ */

    .faq-grid {
      display: grid;
      grid-template-columns: minmax(0, 1.6fr) minmax(0, 1.2fr);
      gap: 18px;
    }

    .faq-item {
      margin-bottom: 10px;
    }

    .faq-q {
      font-size: 13px;
      font-weight: 600;
      margin-bottom: 3px;
    }

    .faq-a {
      font-size: 12px;
      color: var(--muted);
    }

    footer {
      border-top: 1px solid rgba(31, 41, 55, 0.9);
      margin-top: 26px;
      padding-top: 14px;
      font-size: 11px;
      color: var(--muted);
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
      gap: 10px;
    }

    footer span strong {
      color: #e5e7eb;
    }

    @media (max-width: 820px) {
      .hero {
        grid-template-columns: minmax(0, 1fr);
        padding: 18px 16px;
      }
      .hero-title {
        font-size: 24px;
      }
      .hero-visual {
        order: -1;
      }
      .whitepaper-grid,
      .faq-grid,
      .santa-grid {
        grid-template-columns: minmax(0, 1fr);
      }
      .page {
        padding: 18px 12px 40px;
      }
      .header-row {
        flex-direction: column;
        align-items: flex-start;
      }
      .badge-doorway {
        align-self: flex-start;
      }
      .nav-hub {
        margin-top: 8px;
      }
      .countdown-wrap {
        align-items: flex-start;
      }
    }
  </style>
</head>
