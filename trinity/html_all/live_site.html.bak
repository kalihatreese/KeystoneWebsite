<script>
// ---------- Data (example). Add your 50/50 list here. ----------
const storeData = [
  { id: 1, name: "Vintage Lamp", priceUsd: "12.99", priceEth: "0.0065", imageUrl: "images/itemA.png", shippingInfo: "Ships in 3-5 days" },
  { id: 2, name: "Handmade Wallet", priceUsd: "19.50", priceEth: "0.0098", imageUrl: "images/itemB.png", shippingInfo: "Ships in 2-4 days" },
  { id: 999, name: "Free 10 Models (special)", priceUsd: "0.1999", priceEth: "0.00009", imageUrl: "images/special.png", shippingInfo: "Instant digital delivery — pass Reese Truth Test", special:true }
  // add remaining items...
];

// ---------- UI helpers ----------
function $(sel){ return document.querySelector(sel); }
function createEl(tag, attrs={}, html=''){ const el = document.createElement(tag); Object.entries(attrs).forEach(([k,v])=>el.setAttribute(k,v)); if(html) el.innerHTML = html; return el; }

// ---------- Render store ----------
function renderStore(){
  const container = $('#store');
  container.innerHTML = '';
  storeData.forEach(item=>{
    const card = createEl('div',{class:'ks-card','role':'listitem'});
    card.innerHTML = `
      <img src="${item.imageUrl}" alt="${item.name}" onerror="this.src='data:image/svg+xml;utf8,<svg xmlns=\\'http://www.w3.org/2000/svg\\' viewBox=\\'0 0 200 200\\'><rect fill=\\'%23f3f4f6\\' width=\\'200\\' height=\\'200\\'/><text x=\\'50%\\' y=\\'50%\\' dominant-baseline=\\'middle\\' text-anchor=\\'middle\\' fill=\\'%238b8b8b\\' font-family=\\'Arial\\' font-size=\\'16\\'>No image</text></svg>'" />
      <div class="ks-title">${item.name}</div>
      <div class="ks-price">$${item.priceUsd} • ${item.priceEth} ETH</div>
      <div class="ks-shipping">${item.shippingInfo}</div>
      <div class="ks-actions">
        <button class="btn-eth" onclick="handleEthBuy(${item.id})">Buy with ETH</button>
        <a href="https://www.paypal.me/ReeseComp/${item.priceUsd}" target="_blank" rel="noopener"><button class="btn-paypal">PayPal</button></a>
      </div>
    `;
    container.appendChild(card);
  });
}
renderStore();

// ---------- Portal open ----------
$('#store-portal').addEventListener('click', ()=>{
  const s = $('#keystone-store');
  s.style.display = 'block';
  s.setAttribute('aria-hidden','false');
  s.scrollIntoView({behavior:'smooth', block:'center'});
});

// ---------- Reese Effect: randomized, non-repeating immediate ----------
const reeseResponses = [
  "The machine sees your mind, Reese knows.",
  "Chaos creates opportunity.",
  "Time is a loop, act now.",
  "The code never sleeps.",
  "Every trade is a revelation.",
  "Fortune favors the bold.",
  "Energy flows where attention goes.",
  "The keys are within the numbers."
];
// keep last index to avoid immediate repeat in session
let lastReeseIndex = parseInt(sessionStorage.getItem('lastReeseIndex')||'-1',10);
function getRandomReese(){
  let idx;
  do { idx = Math.floor(Math.random()*reeseResponses.length); } while(idx===lastReeseIndex && reeseResponses.length>1);
  lastReeseIndex = idx;
  sessionStorage.setItem('lastReeseIndex', String(idx));
  return reeseResponses[idx];
}
const reeseInput = $('#reeseInput'), reeseOutput = $('#reeseOutput');
if(reeseInput && reeseOutput){
  reeseInput.addEventListener('keypress', (e)=>{
    if(e.key==='Enter'){
      const reply = getRandomReese();
      reeseOutput.textContent = reply;
      reeseOutput.style.color = '#'+Math.floor(Math.random()*16777215).toString(16);
      reeseOutput.style.transform = 'translateY(-8px)';
      setTimeout(()=> reeseOutput.style.transform = 'translateY(0)', 220);
      reeseInput.value = '';
    }
  });
}

// ---------- Reese Truth Test (for special product) ----------
const truthModal = $('#truthModal'), truthBody = $('#truthBody');
let truthResolve = null;

function openTruthModal(itemId){
  truthBody.innerHTML = '';
  // Simple three-step test: phrase match + quick math + choose the codeword
  const phrase = pickRandom(["Freedom not money","Truth through code","Keystone over noise","Serve and build"]);
  const q1 = createEl('div',{},`<div class="truthQuestion">Type exactly: <strong>${phrase}</strong></div><input id="t_phrase" style="padding:8px;width:100%;margin-bottom:10px" placeholder="Type phrase exactly"/>`);
  const a = Math.floor(Math.random()*8)+2;
  const b = Math.floor(Math.random()*8)+2;
  const q2 = createEl('div',{},`<div class="truthQuestion">What is ${a} + ${b} ?</div><input id="t_math" style="padding:8px;width:100%;margin-bottom:10px" placeholder="Answer number"/>`);
  const codewords = shuffleArray(["HONOR","TRUTH","KEYSTONE","REVEAL","LIGHT"]);
  const correctCode = codewords[0];
  let choicesHtml = '<div class="truthQuestion">Choose the correct codeword</div>';
  choicesHtml += '<div class="truthChoices">';
  codewords.slice(0,4).forEach(c=> choicesHtml += `<button class="truthBtn" data-word="${c}" onclick="document.getElementById('t_code').value=this.dataset.word">${c}</button>`);
  choicesHtml += `</div><input id="t_code" placeholder="Selected codeword" style="padding:8px;width:100%;margin-top:6px"/>`;
  const q3 = createEl('div',{},choicesHtml);
  truthBody.appendChild(q1); truthBody.appendChild(q2); truthBody.appendChild(q3);

  const submit = createEl('button',{class:'truthBtn'},'Submit');
  submit.style.marginTop='10px';
  submit.onclick = ()=>{
    const p = document.getElementById('t_phrase').value.trim();
    const m = parseInt(document.getElementById('t_math').value.trim()||'9999',10);
    const c = (document.getElementById('t_code').value||'').trim().toUpperCase();
    if(p !== phrase){ alert('Phrase mismatch — try again'); return; }
    if(m !== (a+b)){ alert('Math answer incorrect'); return; }
    if(c !== correctCode){ alert('Codeword incorrect'); return; }
    // passed
    truthBody.innerHTML = `<div style="padding:12px;background:#ecfdf5;border:1px solid #bbf7d0;border-radius:8px"><strong>Passed ✅</strong> You may proceed to checkout for the special offer.</div>`;
    // resolve promise if waiting
    if(truthResolve) truthResolve(true);
    setTimeout(closeTruthModal,900);
  };
  truthBody.appendChild(submit);
  truthModal.style.display = 'flex';
  truthResolve = null;
  // return a Promise so buyer flow can await
  return new Promise((res)=>{ truthResolve = res; });
}

function closeTruthModal(){ truthModal.style.display='none'; truthBody.innerHTML=''; if(truthResolve) truthResolve(false); truthResolve=null; }

function pickRandom(arr){ return arr[Math.floor(Math.random()*arr.length)]; }
function shuffleArray(a){ const arr=a.slice(); for(let i=arr.length-1;i>0;i--){ const j=Math.floor(Math.random()*(i+1)); [arr[i],arr[j]]=[arr[j],arr[i]] } return arr; }

// ---------- Buy handlers ----------
async function handleEthBuy(itemId){
  const item = storeData.find(s=>s.id===itemId);
  if(!item) return alert('Item not available');
  // if item special, enforce truth test before allowing ETH send
  if(item.special){
    const passed = await openTruthModal(itemId);
    if(!passed) return alert('You must pass the Reese Truth Test to buy this special item.');
  }
  if(!window.ethereum) return alert('MetaMask not detected');
  try{
    const provider = new ethers.providers.Web3Provider(window.ethereum);
    const signer = provider.getSigner();
    const tx = await signer.sendTransaction({ to: '0xc22f0db6ba4935083e9d69a5defd7c32d76228bf', value: ethers.utils.parseEther(item.priceEth) });
    await tx.wait();
    alert('Payment confirmed — thank you!');
    // for digital items or special, you can trigger download/unlock here
  }catch(err){ console.error(err); alert('Payment failed or cancelled'); }
}
</script>
