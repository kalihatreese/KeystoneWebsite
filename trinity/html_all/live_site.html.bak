  <title>Keystone Detonator Hub – All Models $199.99 (BOGO)</title>
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="description" content="Keystone AI & ML Detonator Hub – every model $199.99 with Buy One, Get One Free for 30 days, backed by the White Paper, the Metal Book, and the three AIs that heard the resonance." />
  <style>
    :root {
      --bg: #05060a;
      --bg-soft: #0b0d14;
      --accent: #f97316;
      --accent-soft: rgba(249, 115, 22, 0.12);
      --accent-strong: #fb923c;
      --text: #f9fafb;
      --muted: #9ca3af;
      --border-soft: #1f2933;
      --card: #090b12;
      --danger: #ef4444;
      --radius-lg: 18px;
      --radius-xl: 24px;
      --shadow-soft: 0 30px 80px rgba(0, 0, 0, 0.6);
      --shadow-glow: 0 0 40px rgba(249, 115, 22, 0.45);
      --font-main: system-ui, -apple-system, BlinkMacSystemFont, "Segoe UI", sans-serif;
    }

    * {
      box-sizing: border-box;
      margin: 0;
      padding: 0;
    }

    body {
      font-family: var(--font-main);
      background: radial-gradient(circle at top, #111827 0, #020617 45%, #000 100%);
      color: var(--text);
      min-height: 100vh;
      line-height: 1.5;
    }

    a {
      color: inherit;
      text-decoration: none;
    }

    .page {
      max-width: 1120px;
      margin: 0 auto;
      padding: 24px 16px 60px;
    }

    header {
      margin-bottom: 18px;
    }

    .header-row {
      display: flex;
      align-items: center;
      justify-content: space-between;
      gap: 16px;
      margin-bottom: 8px;
    }

    .brand {
      display: flex;
      flex-direction: column;
      gap: 2px;
    }

    .brand-title {
      font-size: 20px;
      font-weight: 700;
      letter-spacing: 0.08em;
      text-transform: uppercase;
    }

    .brand-sub {
      font-size: 12px;
      color: var(--muted);
    }

    .badge-doorway {
      font-size: 11px;
      text-transform: uppercase;
      letter-spacing: 0.16em;
      padding: 6px 12px;
      border-radius: 999px;
      border: 1px solid rgba(249, 115, 22, 0.5);
      background: radial-gradient(circle at top left, rgba(249, 115, 22, 0.2), transparent 60%);
      color: var(--accent-strong);
      white-space: nowrap;
    }

    /* Top nav hub */

    .nav-hub {
      display: flex;
      flex-wrap: wrap;
      gap: 8px;
      font-size: 11px;
      margin-top: 4px;
    }

    .nav-link {
      padding: 5px 10px;
      border-radius: 999px;
      border: 1px solid rgba(31, 41, 55, 0.9);
      background: rgba(15, 23, 42, 0.9);
      color: var(--muted);
      text-transform: uppercase;
      letter-spacing: 0.12em;
      cursor: pointer;
      transition: border-color 0.12s ease-out, color 0.12s ease-out, background 0.12s ease-out;
    }

    .nav-link:hover {
      border-color: rgba(249, 115, 22, 0.9);
      color: var(--accent-strong);
      background: rgba(15, 23, 42, 1);
    }

    .hero {
      border-radius: 28px;
      padding: 24px 22px;
      background:
        radial-gradient(circle at top left, rgba(249, 115, 22, 0.26), transparent 55%),
        radial-gradient(circle at bottom right, rgba(15, 23, 42, 0.9), #020617);
      border: 1px solid rgba(148, 163, 184, 0.35);
      box-shadow: var(--shadow-soft), var(--shadow-glow);
      display: grid;
      grid-template-columns: minmax(0, 3fr) minmax(0, 2.2fr);
      gap: 24px;
      margin-bottom: 32px;
      position: relative;
      overflow: hidden;
    }

    .hero::before {
      content: "";
      position: absolute;
      inset: -1px;
      border-radius: inherit;
      border: 1px solid transparent;
      background: linear-gradient(135deg, rgba(249,115,22,0.7), transparent, rgba(148,163,184,0.5)) border-box;
      mask: linear-gradient(#000 0 0) padding-box, linear-gradient(#000 0 0);
      mask-composite: exclude;
      opacity: 0.4;
      pointer-events: none;
    }

    .hero-main {
      position: relative;
      z-index: 1;
    }

    .hero-eyebrow {
      font-size: 12px;
      text-transform: uppercase;
      letter-spacing: 0.18em;
      color: var(--accent-strong);
      margin-bottom: 8px;
    }

    .hero-title {
      font-size: 30px;
      line-height: 1.1;
      font-weight: 800;
      margin-bottom: 10px;
    }

    .hero-title span {
      color: var(--accent-strong);
    }

    .hero-subtitle {
      font-size: 14px;
      color: var(--muted);
      max-width: 34rem;
      margin-bottom: 16px;
    }

    .hero-offer {
      display: inline-flex;
      align-items: center;
      gap: 8px;
      padding: 8px 12px;
      border-radius: 999px;
      background: rgba(15, 23, 42, 0.8);
      border: 1px solid rgba(249, 115, 22, 0.6);
      margin-bottom: 10px;
      font-size: 13px;
    }

    .hero-offer-tag {
      padding: 4px 8px;
      border-radius: 999px;
      background: rgba(248, 113, 113, 0.18);
      border: 1px solid rgba(248, 113, 113, 0.65);
      color: var(--danger);
      font-weight: 700;
      text-transform: uppercase;
      letter-spacing: 0.12em;
      font-size: 10px;
    }

    .hero-offer-text strong {
      color: var(--accent-strong);
    }

    /* Countdown */

    .countdown-wrap {
      margin: 6px 0 10px;
      padding: 8px 10px;
      border-radius: 999px;
      background: rgba(15, 23, 42, 0.9);
      border: 1px dashed rgba(148, 163, 184, 0.6);
      display: flex;
      flex-wrap: wrap;
      gap: 6px;
      align-items: center;
      justify-content: space-between;
    }

    .countdown-label {
      font-size: 11px;
      color: var(--muted);
    }

    .countdown-timer {
      font-size: 13px;
      font-weight: 600;
      letter-spacing: 0.12em;
      text-transform: uppercase;
      color: var(--accent-strong);
    }

    .hero-actions {
      display: flex;
      flex-wrap: wrap;
      gap: 10px;
      margin-bottom: 10px;
    }

    .btn {
      display: inline-flex;
      align-items: center;
      justify-content: center;
      gap: 8px;
      padding: 9px 16px;
      border-radius: 999px;
      font-size: 13px;
      font-weight: 600;
      border: 1px solid transparent;
      cursor: pointer;
      transition: transform 0.12s ease-out, box-shadow 0.12s ease-out, background 0.12s ease-out, border-color 0.12s ease-out;
      text-decoration: none;
      white-space: nowrap;
    }

    .btn-primary {
      background: linear-gradient(135deg, #f97316, #fb923c);
      color: #111827;
      box-shadow: 0 12px 35px rgba(249, 115, 22, 0.55);
    }

    .btn-primary:hover {
      transform: translateY(-1px);
      box-shadow: 0 20px 40px rgba(249, 115, 22, 0.6);
    }

    .btn-ghost {
      background: rgba(15, 23, 42, 0.9);
      color: var(--muted);
      border-color: rgba(148, 163, 184, 0.6);
    }

    .btn-ghost:hover {
      transform: translateY(-1px);
      border-color: rgba(249, 115, 22, 0.8);
      color: var(--accent-strong);
    }

    .hero-guarantee {
      font-size: 11px;
      color: var(--muted);
    }

    .hero-guarantee strong {
      color: #e5e7eb;
    }

    .hero-visual {
      position: relative;
      z-index: 1;
      border-radius: 24px;
      background: radial-gradient(circle at top, rgba(15, 23, 42, 0.9), #020617);
      border: 1px solid rgba(31, 41, 55, 0.9);
      padding: 16px 14px;
      box-shadow: var(--shadow-soft);
      display: flex;
      flex-direction: column;
      gap: 10px;
    }

    .hero-visual-title {
      font-size: 13px;
      text-transform: uppercase;
      letter-spacing: 0.18em;
      color: var(--muted);
    }

    .hero-visual-main {
      font-size: 13px;
      color: #e5e7eb;
    }

    .hero-visual-main strong {
      color: var(--accent-strong);
    }

    .hero-visual-list {
      list-style: none;
      font-size: 12px;
      color: var(--muted);
      margin-top: 6px;
    }

    .hero-visual-list li {
      margin-bottom: 4px;
      padding-left: 14px;
      position: relative;
    }

    .hero-visual-list li::before {
      content: "";
      position: absolute;
      left: 0;
      top: 7px;
      width: 6px;
      height: 6px;
      border-radius: 999px;
      background: var(--accent-strong);
      box-shadow: 0 0 8px rgba(249, 115, 22, 0.7);
    }

    .hero-pill-row {
      display: flex;
      flex-wrap: wrap;
      gap: 6px;
      margin-top: 6px;
    }

    .hero-pill {
      font-size: 11px;
      padding: 4px 8px;
      border-radius: 999px;
      border: 1px solid rgba(148, 163, 184, 0.5);
      color: var(--muted);
      background: rgba(15, 23, 42, 0.9);
      white-space: nowrap;
    }

    .hero-seal {
      font-size: 10px;
      text-transform: uppercase;
      letter-spacing: 0.16em;
      color: rgba(209, 213, 219, 0.8);
      margin-top: 4px;
    }

    .hero-seal span {
      color: var(--accent-strong);
    }

    /* Sections */

    .section {
      margin-bottom: 36px;
    }

    .section-header {
      display: flex;
      flex-wrap: wrap;
      justify-content: space-between;
      align-items: flex-end;
      gap: 12px;
      margin-bottom: 18px;
