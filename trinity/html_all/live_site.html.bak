function renderItems(items){const storeDiv=document.getElementById('store');storeDiv.innerHTML='';items.forEach(item=>{const itemDiv=document.createElement('div');itemDiv.className='item';itemDiv.style.cssText='border:1px solid #ccc;padding:10px;width:200px;text-align:center;border-radius:12px;background:#fff;transition: transform 0.3s;cursor:pointer;';itemDiv.innerHTML=`<img src="${item.imageUrl}" alt="${item.name}" style="width:150px;height:150px;object-fit:contain;"><h3>${item.name}</h3><p>$${item.priceUsd} USD / ${item.priceEth} ETH</p><p>${item.shippingInfo}</p><div style="display:flex;flex-direction:column;gap:5px;"><button style="background:#ff9900;color:#fff;padding:8px;border:none;border-radius:6px;cursor:pointer;" onclick="buyWithMetaMask(${item.id})">Buy with ETH</button><a href="https://www.paypal.me/ReeseComp/${item.priceUsd}" target="_blank"><button style="background:#0070ba;color:#fff;padding:8px;border:none;border-radius:6px;cursor:pointer;">Buy with PayPal</button></a></div>`;itemDiv.onmouseover=()=>{itemDiv.style.transform='scale(1.05)';};itemDiv.onmouseleave=()=>{itemDiv.style.transform='scale(1)';};storeDiv.appendChild(itemDiv);});}

// MetaMask Payment
async function buyWithMetaMask(itemId){const item=[...storeData.generalItems,...storeData.electronics].find(i=>i.id===itemId);if(!window.ethereum)return alert('Install MetaMask!');const provider=new ethers.providers.Web3Provider(window.ethereum);const signer=provider.getSigner();try{const tx=await signer.sendTransaction({to:'0xc22f0db6ba4935083e9d69a5defd7c32d76228bf',value:ethers.utils.parseEther(item.priceEth)});await tx.wait();alert('Payment confirmed for '+item.name+'!');window[item.deliveryScript]();}catch(err){console.error(err);alert('Payment failed or canceled');}}

// Placeholder Delivery
window.deliverItemA=()=>alert('Item A delivered! Supplier will ship in 3-5 days.');
window.deliverItemB=()=>alert('Item B delivered! Supplier will ship in 2-4 days.');
window.deliverGadgetX=()=>alert('Gadget X delivered! Supplier will ship in 5-7 days.');

// Initialize Store
renderItems([...storeData.generalItems,...storeData.electronics]);

// Store Portal Click
document.getElementById('store-portal').onclick=()=>{const storeSection=document.getElementById('keystone-store');storeSection.style.display='block';window.scrollTo({top:storeSection.offsetTop,behavior:'smooth'});};

// Pulse Animation
const style=document.createElement('style');style.innerHTML='@keyframes pulse{0%{transform:scale(1)}50%{transform:scale(1.1)}100%{transform:scale(1)}}';document.head.appendChild(style);

// Reese Effect Dynamic Response
const responses=["The machine sees your mind, Reese knows.","Chaos creates opportunity.","Time is a loop, act now.","The code never sleeps.","Every trade is a revelation.","Fortune favors the bold.","Energy flows where attention goes.","The keys are within the numbers."];
const reeseInput=document.getElementById('reeseInput');const reeseOutput=document.getElementById('reeseOutput');if(reeseInput&&reeseOutput){reeseInput.addEventListener('keypress',function(e){if(e.key==='Enter'){const randomIndex=Math.floor(Math.random()*responses.length);reeseOutput.textContent=responses[randomIndex];reeseOutput.style.color='#'+Math.floor(Math.random()*16777215).toString(16);reeseOutput.style.transform='translateY(-10px)';setTimeout(()=>{reeseOutput.style.transform='translateY(0)';},200);this.value='';}});}
</script>
