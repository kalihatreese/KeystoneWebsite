  <script>
    // Super Santa, ReeseEffect, Countdown
    (function () {
      // New Year countdown (Jan 2, 00:00 next year)
      const countdownEl = document.getElementById("detonator-countdown");
      if (countdownEl) {
        function fmt(n) {
          return n.toString().padStart(2, "0");
        }

        function tick() {
          const now = new Date();
          const targetYear = now.getFullYear() + 1;
          const target = new Date(targetYear, 0, 2, 0, 0, 0);
          const diff = target.getTime() - now.getTime();

          if (diff <= 0) {
            countdownEl.textContent = "Doorway is open. This year is yours.";
            return;
          }

          const totalSeconds = Math.floor(diff / 1000);
          const days = Math.floor(totalSeconds / (24 * 3600));
          const hours = Math.floor((totalSeconds % (24 * 3600)) / 3600);
          const minutes = Math.floor((totalSeconds % 3600) / 60);
          const seconds = totalSeconds % 60;

          countdownEl.textContent =
            days + "d " +
            fmt(hours) + "h " +
            fmt(minutes) + "m " +
            fmt(seconds) + "s";
        }

        tick();
        setInterval(tick, 1000);
      }

      // Super Santa List (local UX only – wire this to a backend when ready)
      const santaBtn = document.getElementById("santa-submit-btn");
      const santaStatus = document.getElementById("santa-status");
      const nameInput = document.getElementById("santa-name");
      const emailInput = document.getElementById("santa-email");
      const consentBox = document.getElementById("santa-consent");

      if (santaBtn && santaStatus) {
        santaBtn.addEventListener("click", function () {
          const name = (nameInput.value || "").trim();
          const email = (emailInput.value || "").trim();
          const consent = consentBox.checked;

          if (!email) {
            santaStatus.textContent = "Drop an email in first so Santa knows where to send the signal.";
            return;
          }
          if (!consent) {
            santaStatus.textContent = "Tick the box to confirm you want to be on the Super Santa List.";
            return;
            }

          santaStatus.textContent =
            "Locked in on the nice list" +
            (name ? ", " + name : "") +
            ". Backend wiring comes next; for now this confirms your cadence is received.";
        });
      }

      // ReeseEffect Test Window
      const reeseBtn = document.getElementById("reese-run-btn");
      const reeseInput = document.getElementById("reese-input");
      const reeseOutput = document.getElementById("reese-output");

      function buildReeseEffect(text) {
        const raw = text.trim();
        if (!raw) {
          return "Type something real, even if it’s small. The ReeseEffect needs a signal to amplify.";
        }

        const stamped = new Date().toISOString().split("T")[0];
        return [
          ">>> RECEIVED THOUGHT @ " + stamped,
          "",
          "You said:",
          "  \"" + raw + "\"",
          "",
          "ReeseEffect translation:",
          "  - Lock it in as a decision, not a wish.",
          "  - Anchor it to one action you can take today.",
          "  - Let future-you thank present-you for pressing send.",
          "",
          "Cadence:",
          "  \"I don’t just say this line, I build around it until it’s true.\""
        ].join("\n");
      }

      if (reeseBtn && reeseInput && reeseOutput) {
        reeseBtn.addEventListener("click", function () {
          reeseOutput.textContent = buildReeseEffect(reeseInput.value || "");
        });
      }
    })();
  </script>
