      font-weight: 800;
    }

    .model-price span {
      font-size: 11px;
      font-weight: 600;
      color: var(--muted);
      margin-left: 4px;
    }

    .model-bogo {
      font-size: 11px;
      color: var(--accent-strong);
      text-transform: uppercase;
      letter-spacing: 0.12em;
    }

    .model-actions {
      margin-top: 6px;
      display: flex;
      justify-content: space-between;
      gap: 8px;
      align-items: center;
      flex-wrap: wrap;
    }

    .btn-model {
      flex: 1;
      text-align: center;
      padding: 7px 10px;
      font-size: 12px;
      border-radius: 999px;
      border: 1px solid rgba(249, 115, 22, 0.7);
      background: linear-gradient(135deg, rgba(249, 115, 22, 0.1), rgba(249, 115, 22, 0.3));
      color: var(--accent-strong);
      font-weight: 600;
      cursor: pointer;
      transition: transform 0.12s ease-out, box-shadow 0.12s ease-out, background 0.12s;
    }

    .btn-model:hover {
      transform: translateY(-1px);
      box-shadow: 0 12px 30px rgba(249, 115, 22, 0.4);
      background: linear-gradient(135deg, rgba(249, 115, 22, 0.2), rgba(249, 115, 22, 0.5));
    }

    .model-note {
      font-size: 10px;
      color: var(--muted);
      max-width: 120px;
      text-align: right;
    }

    /* Whitepaper / Metal Book */

    .whitepaper-grid {
      display: grid;
      grid-template-columns: minmax(0, 1.5fr) minmax(0, 1.3fr);
      gap: 18px;
    }

    .card {
      background: var(--bg-soft);
      border-radius: var(--radius-xl);
      border: 1px solid rgba(31, 41, 55, 0.9);
      padding: 16px 15px;
      box-shadow: var(--shadow-soft);
    }

    .card-title {
      font-size: 15px;
      font-weight: 700;
      margin-bottom: 6px;
    }

    .card-sub {
      font-size: 12px;
      color: var(--muted);
      margin-bottom: 10px;
    }

    .bullet-list {
      list-style: none;
      font-size: 12px;
      color: #e5e7eb;
      margin-bottom: 10px;
    }

    .bullet-list li {
      margin-bottom: 4px;
      padding-left: 12px;
      position: relative;
    }

    .bullet-list li::before {
